{
    "name": "juzaweb/installer",
    "description": "Juzaweb CMS installer",
    "keywords": [
        "Juzaweb CMS",
        "installer"
    ],
    "license": "MIT",
    "homepage": "https://cms.juzaweb.com",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0",
        "laravel/pint": "^1.13",
        "juzaweb/core": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Installer\\": "src/"
        },
        "files": [
            "helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\Installer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juzaweb\\Installer\\Providers\\InstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}